<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">
<urlrewrite>
    <rule>
        <from>^/tags/(.*).html$</from>
        <to>/tags/$1</to>
    </rule>
    <rule>
        <from>^/article/(.*).html$</from>
        <to>/article/$1</to>
    </rule>
</urlrewrite>